# ~/.bash_aliases

# Set some local to user aliases
alias 2svn='cd $SVN'
alias mesa='cd $MESA_HOME'
alias emacs='emacs -nw'
alias shared='cd $SHARE_APPS'
alias intel='cd $SHARE_APPS/intel'
alias lib='cd $MESA_HOME/library'
alias tim='cd $MESA_HOME/Time_Propagation'
alias bmesa='cd $MESA_HOME/bmesa'
alias run='cd $MESA_RUN'
alias scratch='cd $MESA_RUN/mesa_temp'
alias usr='cd $SHARE_USR'
alias rep=$REPOSITORY
alias ls='ls --color=always'
