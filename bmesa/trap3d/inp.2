$route
 print=(eigen=xall) m7000=xcheck-orthogonality number-of-dimensions=3
 diagonalize-h0  xuse-atomic-units iterative-diagonalization xto-atomic-units
 number-of-roots=5 in-coordinate-space
$end
          
$nonstd
70//00;
20//01;
$end
$title
3-d trap code
$end
$trap
 x-trap-frequency=10.d0 y-trap-frequency=10.d0 z-trap-frequency=10.d0
 minimum-number-of-atoms=0. maximum-number-of-atoms=0. number-of-steps=1
$end
$x-polynomials
 order-of-polynomials=5,number-of-points=7,fix-end-points
 number-of-fixed-points=2, left=-2.5d-05 right=2.5d-05
 xleft=-0.d0 xright=15.d0
 order-of-leading-left-polynomials=1 order-of-leading-right-polynomials=1
$end
$y-polynomials
 order-of-polynomials=5,number-of-points=7,fix-end-points
 number-of-fixed-points=2, left=-2.5d-05 right=2.5d-05
 xleft=0.d0 xright=15.d0
 order-of-leading-left-polynomials=1 order-of-leading-right-polynomials=1
$end
$z-polynomials
 order-of-polynomials=5,number-of-points=7,fix-end-points
 number-of-fixed-points=2, left=-2.5d-05 right=2.5d-05
 xleft=0.d0 xright=15.d0
 order-of-leading-left-polynomials=1 order-of-leading-right-polynomials=1
$end
$eigen
 number-of-roots=10 number-of-trial-vectors=10 to-h0 convergence=1.d-05
 xprint=buffered-hamiltonian xprint=trials 
$end
$v0
 unperturbed-potential-type=harmonic-oscillator diagonal-potential
 no-off-diagonal-potential
 a11=-1.d0 s11=1.d0 omega-11=10.d0 omega-22=10.d0
$end 
$v1
 interaction-potential-type=harmonic-oscillator diagonal-potential
 no-off-diagonal-potential omega-11=10.d0 omega-22=10.d0 omega-33=10.d0
 a11=-1.d0 s11=1.d0 
 a22=-1.d0 s22=1.d0  
$end 
 
